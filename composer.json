{
  "name": "brianjhanson/craft-super-text",
  "description": "Additional features for text fields in Craft CMS",
  "type": "craft-plugin",
  "version": "1.0.0",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "text tag"
  ],
  "support": {
    "docs": "https://github.com/brianjhanson/craft-super-text/blob/main/README.md",
    "issues": "https://github.com/brianjhanson/craft-super-text/issues"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Brian Hanson",
      "homepage": "https://brianhanson.net"
    }
  ],
  "require": {
    "craftcms/cms": "^3.2.0"
  },
  "require-dev": {
    "codeception/codeception": "^4.0",
    "vlucas/phpdotenv": "^3.0",
    "codeception/module-yii2": "^1.0.0",
    "codeception/module-asserts": "^1.0.0"
  },
  "autoload": {
    "psr-4": {
      "brianjhanson\\supertext\\": "src/",
      "brianjhanson\\supertexttests\\fixtures\\": "tests/fixtures/"
    }
  },
  "extra": {
    "name": "Super Text",
    "handle": "super-text",
    "developer": "Brian Hanson",
    "developerUrl": "https://brianhanson.net",
    "documentationUrl": "https://github.com/brianjhanson/craft-super-text/blob/main/README.md",
    "changelogUrl": "https://raw.githubusercontent.com/brianjhanson/craft-super-text/main/CHANGELOG.md",
    "class": "brianjhanson\\supertext\\SuperText"
  }
}
